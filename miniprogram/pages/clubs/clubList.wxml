<!--miniprogram/pages/clubs/clubList.wxml-->

<view 
	wx:for="{{clubs}}"
  	wx:key="{item.id}"
	class="uploader">

	<navigator url="../matches/matchList?clubid={{item.id}}" open-type="navigate" class="uploader-text">
    <text>{{item.wholeName}}</text>
    <text> ({{item.shortName}})</text>
    <text class="textalignend, debug">{{item.createDate}}</text>
	</navigator>
</view>