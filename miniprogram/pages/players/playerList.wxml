<!--miniprogram/pages/players/playerList.wxml-->
<view 
	wx:for="{{players}}"
  	wx:key="{item.id}"
	class="uploader">

	<navigator url="../players/detail?id={{item.id}}" open-type="navigate" class="uploader-text">
    <image class="checkbox" src="../../images/icon-emptybox.svg" mode="aspectFit"></image>
	  <text>{{item.name}}</text>
	</navigator>
</view>