<!--miniprogram/pages/matchList/matchList.wxml-->

<view class="userinfo-nickname-wrapper">
      <button class="userinfo-nickname" bindtap="onNewGame">点击新建</button>
    </view>

<view 
	wx:for="{{matches}}"
  	wx:key="{item.id}"
	class="uploader">

	<navigator url="../matches/detail?action=old&clubid={{item.clubid}}&matchid={{item._id}}" open-type="navigate" class="uploader-text">
	  <text>{{item.createDate}}</text>
	</navigator>
</view>

